---
eleventyExcludeFromCollections: true
permalink: /feed.xml
eleventyImport:
  collections: ["post"]
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:base="https://velvetcache.org">
  <title>jmhobbs Blog</title>
  <subtitle>I do computer stuff.</subtitle>
  <link href="{{ permalink | absoluteUrl("https://velvetcache.org/") }}" rel="self"/>
  <link href="https://velvetcache.org/"/>
  <updated>{{ collections.post | getNewestCollectionItemDate | dateToRfc3339 }}</updated>
  <id>https://velvetcache.org/</id>
  <author>
    <name>John Hobbs</name>
    <email>john@velvetcache.org</email>
  </author>
  {%- for post in collections.post | reverse %}
  {%- set absolutePostUrl = post.url | absoluteUrl("https://velvetcache.org/") %}
  <entry>
    <title>{{ post.data.title }}</title>
    <link href="{{ absolutePostUrl }}"/>
    <updated>{{ post.date | dateToRfc3339 }}</updated>
    <id>{{ absolutePostUrl }}</id>
    <summary>{{ post.data.summary }}</summary>
  </entry>
  {%- endfor %}
</feed>
